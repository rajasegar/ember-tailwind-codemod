{
  "name": "ember-tailwind-codemod",
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint --cache .",
    "test": "codemod-cli test --coverage",
    "test:coverage": "codemod-cli test --coverage",
    "update-docs": "codemod-cli update-docs",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "copy": "cp ../super-rentals/app/components/*.hbs ./demo/templates && cp ../super-rentals/app/templates/*.hbs ./demo/templates && cp ../super-rentals/dist/assets/*.css ./demo/css"
  },
  "bin": "./bin/cli.js",
  "keywords": [
    "codemod-cli"
  ],
  "dependencies": {
    "codemod-cli": "^3.1.2",
    "ember-template-recast": "^5.0.3",
    "postcss": "^8.2.15"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0"
  },
  "engines": {
    "node": "10.* || 12.* || >= 14"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
